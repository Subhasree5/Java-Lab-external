class MorningThread implements Runnable{
    public void run(){
        try{
            System.out.println("Good Mornig");
            Thread.sleep(1000);
        }catch(Exception e){
            System.out.println(e);
        }
    }
}
class HelloThread implements Runnable{
    public void run(){
        try{
            System.out.println("Hello");
            Thread.sleep(2000);
        }catch(Exception e){
            System.out.println(e);
        }
    }
}
class WelcomeThread implements Runnable{
    public void run(){
        try{
            System.out.println("Welcome");
            Thread.sleep(3000);
        }catch(Exception e){
            System.out.println(e);
        }
    }
}
public class Main{
    public static void main(String[]args){
        Thread t1=new Thread(new MorningThread());
        Thread t2=new Thread(new HelloThread());
        Thread t3=new Thread(new WelcomeThread());
        t2.start();
        t1.start();
        t3.start();
    }
}
Good Mornig
Welcome
Hello
