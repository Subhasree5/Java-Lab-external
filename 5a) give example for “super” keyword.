class Super{
    float salary=10000;
}
class child extends Super{
    float salary=20000;
    void display(){
        System.out.println("Child= "+salary);
        System.out.println("Parent= "+super.salary);
    }
}
public class Main{
    public static void main(String[]args){
        child obj=new child();
        obj.display();
    }
}
Child= 20000.0
Parent= 10000.0
